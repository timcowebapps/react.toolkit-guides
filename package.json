{
	"name": "@timcowebapps/react.toolkit-guides",
	"version": "0.4.12",
	"description": "Front-end фреймворк для разработки адаптивных web-проектов",
	"author": "Victor Timoshin <victor-timoshin@hotmail.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/timcowebapps/react.toolkit-guides"
	},
	"scripts": {
		"preclean": "rm -rf docs/*",
		"postclean": "rm -f docs/*.d.ts && rm -rf docs/views/*",
		"copyfiles": "cp 404.html ./docs/ && cp src/views/partials/gist/iframe.css ./docs/",
		"build": "npm run preclean && NODE_ENV=production webpack --display-used-exports --hide-modules && npm run postclean && npm run copyfiles",
		"start": "npm run preclean && webpack-dev-server --hot --inline --content-base ./docs"
	},
	"dependencies": {
		"lodash": "^4.17.11",
		"prop-types": "^15.6.2",
		"react": "^16.6.3",
		"react-dom": "^16.6.3",
		"react-router-dom": "^4.3.1"
	},
	"devDependencies": {
		"@timcowebapps/react.component": "^0.3.3",
		"@timcowebapps/react.componentmodel": "^0.1.4",
		"@timcowebapps/react.style": "^0.3.5",
		"@timcowebapps/react.system": "^0.2.5",
		"@timcowebapps/react.toolkit": "^0.45.82",
		"@timcowebapps/react.utils": "^0.14.61",
		"@types/lodash": "^4.14.119",
		"@types/node": "^10.12.18",
		"@types/react": "^16.7.17",
		"@types/react-dom": "^16.0.11",
		"@types/react-router-dom": "^4.3.1",
		"css-loader": "^2.0.1",
		"extract-text-webpack-plugin": "^4.0.0-beta.0",
		"html-webpack-plugin": "^3.2.0",
		"node-sass": "^4.11.0",
		"path": "^0.12.7",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.23.1",
		"ts-loader": "^5.3.1",
		"ts-node": "^7.0.1",
		"typescript": "^3.2.2",
		"uglifyjs-webpack-plugin": "^2.0.1",
		"webpack": "^4.28.0",
		"webpack-cli": "^3.1.2",
		"webpack-dev-server": "^3.1.10"
	}
}
